# /etc/grafana/provisioning/datasources - путь в контейнере
добавить datasource.yml

# содержимое файла
apiVersion: 1

datasources:
  - name: Prometheus
    type: prometheus
    access: proxy
    url: http://prometheus:9090
    isDefault: true
    editable: true

# в докер композ :
- ./datasource.yml:/etc/grafana/provisioning/datasources/datasource.yml

